<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-feature android:name="android.hardware.sensor.accelerometer" android:required="true" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Btm_menu_test"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalenderActivity"
            android:theme="@style/CustomActivityTheme1" />
        <activity android:name=".LinkActivity" />
        <activity android:name=".ImageActivity"
            android:theme="@style/CustomActivityTheme1"/>
        <activity android:name=".TipActivity"
            android:theme="@style/CustomActivityTheme1"/>
        <activity android:name=".SupplementsActivity"
            android:theme="@style/CustomActivityTheme1"/>
        <activity android:name=".EggSub"/>
        <activity android:name=".GameActivity"/>
        <activity android:name=".eggTouchGame"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>